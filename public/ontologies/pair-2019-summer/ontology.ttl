#################################################################
#    Classes
#################################################################

###  http://virtual-assembly.org/ontologies/pair#Address
:Address rdf:type owl:Class ;
         rdfs:subClassOf :WhereSubject ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasPart ;
                           owl:allValuesFrom :Town
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :localizes ;
                           owl:allValuesFrom [ rdf:type owl:Class ;
                                               owl:unionOf ( :Event
                                                             :Person
                                                             :Place
                                                           )
                                             ]
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasPart ;
                           owl:cardinality "1"^^xsd:nonNegativeInteger
                         ] ;
         rdfs:label "Address"@en ,
                    "Adresse"@fr .


###  http://virtual-assembly.org/ontologies/pair#Building
:Building rdf:type owl:Class ;
          rdfs:subClassOf :BuildingItem ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasPart ;
                            owl:allValuesFrom :Floor
                          ] ;
          rdfs:label "Building"@en ,
                     "Immeuble"@fr .


